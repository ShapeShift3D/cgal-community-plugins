<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy
      name="CGAL2DConstrainedDelaunayTriangulationMesher"
      class="stkCGAL2DConstrainedDelaunayTriangulationMesher"
      label="CGAL 2D Constrained Delaunay Triangulation Mesher">

      <Documentation
        short_help="Generates 2D mesh from 2D polylines."
        long_help="Generates 2D mesh from 2D polylines.">
        This filter generates a 2D mesh from 2D polylines defining the boundaries. It takes a
        PolyData as input, and returns a PolyData.
      </Documentation>

      <InputProperty
        name="Input"
        port_index="0"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData" />
        </DataTypeDomain>
        <Documentation>
          The input Mesh.
        </Documentation>
      </InputProperty>

      <Hints>
        <ShowInMenu category="CGAL" />
      </Hints>
      
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>