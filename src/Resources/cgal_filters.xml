        <SourceProxy name="CGALRegionGrowing"
               class="stkCGALRegionGrowing"
               label="CGAL Shape Detection - Region Growing">
            <Documentation long_help="todo."
                       short_help="todo."> </Documentation>
            <InputProperty name="Input"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
            </InputProperty>
            <IntVectorProperty name="Kernel"
                               command="SetKernelValue"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry text="EPEC"              value="1" />
                    <Entry text="EPEC_SQRT"         value="2" />
                    <Entry text="EPEC_KTH_ROOT"     value="3" />
                    <Entry text="EPEC_ROOT_OF"      value="4" />
                    <Entry text="EPIC"              value="5" />
                    <Entry text="Cartesian"         value="6" />
                    <Entry text="Simple_cartesian"  value="7" />
                </EnumerationDomain>
                <Documentation>
					Set the kernel. Depending on the CGAL kernel used,
                    the compute time may very a lot and so the output regions.
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty name="MaxDistanceToPlane"
                               command="SetMaxDistanceToPlane"
                               number_of_elements="1"
                               default_values="1.0">
                <DoubleRangeDomain min="0.0" name="range" />
                <Documentation>
                    The maximum distance from the furthest face vertex to a plane.
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty name="MaxAcceptedAngle"
                               command="SetMaxAcceptedAngle"
                               number_of_elements="1"
                               default_values="45.0">
                <Documentation>
                    The maximum accepted angle between the face normal and the normal of a plane.
                </Documentation>
            </DoubleVectorProperty>
            <IntVectorProperty name="MinRegionSize"
                               command="SetMinRegionSize"
                               number_of_elements="1"
                               default_values="5">
                <IntRangeDomain min="0" name="range" />
                <Documentation>
                    The minimum number of mesh faces a region must have.
                </Documentation>
            </IntVectorProperty>
		    <Hints> 
                <ShowInMenu category="CGAL"/>
            </Hints>
