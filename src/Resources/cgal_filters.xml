        <!-- ==================================================================== -->
        <SourceProxy
            name="IsotropicRemeshingFilter"
            class="stkCGALIsotropicRemeshingFilter"
            label="CGAL Isotropic Remeshing">
            <Documentation
                short_help="Remeshes datasets."
                long_help="Remeshes a dataset.">
                This filter will remesh the given data set. It takes a PolyData 
                as input, and returns a PolyData containing the remeshed input.
                An optional input array can be supplied to mask the remeshing zone.
            </Documentation>
            <InputProperty
                name="Input"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData" />
                </DataTypeDomain>
                <InputArrayDomain
                    name="MeshingMaskArray"
                    attribute_type="cell"
                    number_of_components="1"
                    optional="1" />
                <Documentation>
                    The input Mesh.
                </Documentation>
            </InputProperty>
            <StringVectorProperty
                name="MeshingMaskArrayName"
                command="SetMeshingMaskArrayName"
                label="Meshing Mask"
                default_values="None"
                number_of_elements="1">
                <ArrayListDomain
                    name="array_list"
                    none_string="None"
                    input_domain_name="MeshingMaskArray">
                    <RequiredProperties>
                        <Property
                            name="Input"
                            function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Specifies the meshing mask array name.
                </Documentation>
            </StringVectorProperty>
            <IntVectorProperty
                name="ProtectConstraints"
                command="SetProtectConstraints"
                default_values="1"
                number_of_elements="1">
                <Documentation>
                    Toggles border constraints protection.
                    The boundary edges will be considered as constrained and protected.
                </Documentation>
                <BooleanDomain name="bool" />
            </IntVectorProperty>
            <DoubleVectorProperty
                name="TargetEdgeLength"
                command="SetTargetEdgeLength"
                number_of_elements="1"
                default_values="0.0"
                information_property="TargetEdgeLengthInfo">
                <DoubleRangeDomain
                    name="range"
                    min="0.0" />
                <Documentation>
                    The target length for the new edges of the mesh (default is 1% of the bounding box diagonal).
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty
                name="TargetEdgeLengthInfo"
                command="GetTargetEdgeLengthInfo"
                information_only="1">
                <SimpleDoubleInformationHelper />
            </DoubleVectorProperty>
            <IntVectorProperty
                name="NumberOfIterations"
                command="SetNumberOfIterations"
                number_of_elements="1"
                default_values="1">
                <IntRangeDomain
                    name="range"
                    min="1" />
                <Documentation>
                    The number of iterations for the sequence of atomic operations performed (edge splits,
                    edge collapses, edge flips, tangential relaxation and projection to the initial surface
                    to generate a smooth mesh with a prescribed edge length).
                </Documentation>
            </IntVectorProperty>
            <Hints>
                <ShowInMenu category="CGAL" />
            </Hints>
        </SourceProxy>
        <!-- ==================================================================== -->
        <SourceProxy
            name="CGALRegionGrowing"
            class="stkCGALRegionGrowing"
            label="CGAL Shape Detection - Region Growing">
            <Documentation
                long_help="todo."
                short_help="todo." />
            <InputProperty
                name="Input"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet" />
                </DataTypeDomain>
            </InputProperty>
            <IntVectorProperty
                name="Kernel"
                command="SetKernelValue"
                number_of_elements="1"
                default_values="1">
                <EnumerationDomain name="enum">
                    <Entry
                        text="EPEC"
                        value="1" />
                    <Entry
                        text="EPEC_SQRT"
                        value="2" />
                    <Entry
                        text="EPEC_KTH_ROOT"
                        value="3" />
                    <Entry
                        text="EPEC_ROOT_OF"
                        value="4" />
                    <Entry
                        text="EPIC"
                        value="5" />
                    <Entry
                        text="Cartesian"
                        value="6" />
                    <Entry
                        text="Simple_cartesian"
                        value="7" />
                </EnumerationDomain>
                <Documentation>
					Set the kernel. Depending on the CGAL kernel used,
                    the compute time may very a lot and so the output regions.
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty
                name="MaxDistanceToPlane"
                command="SetMaxDistanceToPlane"
                number_of_elements="1"
                default_values="1.0">
                <DoubleRangeDomain
                    min="0.0"
                    name="range" />
                <Documentation>
                    The maximum distance from the furthest face vertex to a plane.
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty
                name="MaxAcceptedAngle"
                command="SetMaxAcceptedAngle"
                number_of_elements="1"
                default_values="45.0">
                <Documentation>
                    The maximum accepted angle between the face normal and the normal of a plane.
                </Documentation>
            </DoubleVectorProperty>
            <IntVectorProperty
                name="MinRegionSize"
                command="SetMinRegionSize"
                number_of_elements="1"
                default_values="5">
                <IntRangeDomain
                    min="0"
                    name="range" />
                <Documentation>
                    The minimum number of mesh faces a region must have.
                </Documentation>
            </IntVectorProperty>
            <Hints>
                <ShowInMenu category="CGAL" />
            </Hints>
        </SourceProxy>
        <!-- ==================================================================== -->
        <SourceProxy
            name="CGALEfficientRANSAC"
            class="stkCGALEfficientRANSAC"
            label="CGAL Shape Detection - Efficient RANSAC">
            <Documentation
                long_help="todo."
                short_help="todo." />
            <InputProperty
                name="Input"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet" />
                </DataTypeDomain>
            </InputProperty>
            <IntVectorProperty
                name="NumberOfIterations"
                command="SetNumberOfIterations"
                number_of_elements="1"
                default_values="1">
                <IntRangeDomain
                    min="1"
                    name="range" />
                <Documentation>
                    The algorithm will perform detection several times
                    and choose resultwith the highest coverage
                    (i.e. ratio of the points assigned to a shape).
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty
                name="UserDefinedParameters"
                command="SetUserDefinedParameters"
                number_of_elements="1"
                default_values="0">
                <BooleanDomain name="bool" />
                <Documentation>
                    If not checked, the algorithm will evaluate default
                    values based on the mesh parameters (size, ...).
                    Otherwise the user has the ability define detection parameters.
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty
                name="MinPoints"
                command="SetMinPoints"
                number_of_elements="1"
                default_values="200">
                <IntRangeDomain
                    min="0"
                    name="range" />
                <Hints>
                    <PropertyWidgetDecorator
                        type="GenericDecorator"
                        mode="visibility"
                        property="UserDefinedParameters"
                        value="1" />
                </Hints>
                <Documentation>
                    Set the minimum number of points in a shape.
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty
                name="Probability"
                command="SetProbability"
                number_of_elements="1"
                default_values="0.05">
                <DoubleRangeDomain
                    min="0.0"
                    max="1.0"
                    name="range" />
                <Hints>
                    <PropertyWidgetDecorator
                        type="GenericDecorator"
                        mode="visibility"
                        property="UserDefinedParameters"
                        value="1" />
                </Hints>
                <Documentation>
                    Set the probability to miss the largest primitive at each iteration. Probability to control search endurance.
                    A lower probability provides a higher reliability and determinism at the cost of longer running time due to a higher search endurance.
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty
                name="Epsilon"
                command="SetEpsilon"
                number_of_elements="1"
                default_values="0.002">
                <DoubleRangeDomain
                    min="0.0"
                    name="range" />
                <Hints>
                    <PropertyWidgetDecorator
                        type="GenericDecorator"
                        mode="visibility"
                        property="UserDefinedParameters"
                        value="1" />
                </Hints>
                <Documentation>
                    Set the maximum acceptable Euclidean distance between a point and a shape.
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty
                name="ClusterEpsilon"
                command="SetClusterEpsilon"
                number_of_elements="1"
                default_values="0.01">
                <DoubleRangeDomain
                    min="0.0"
                    name="range" />
                <Hints>
                    <PropertyWidgetDecorator
                        type="GenericDecorator"
                        mode="visibility"
                        property="UserDefinedParameters"
                        value="1" />
                </Hints>
                <Documentation>
                    Set the maximum acceptable Euclidean distance between points, which are assumed to be neighbors.
                </Documentation>
            </DoubleVectorProperty>
            <DoubleVectorProperty
                name="MaxNormalDeviation"
                command="SetMaxNormalDeviation"
                number_of_elements="1"
                default_values="0.9">
                <DoubleRangeDomain
                    min="0.0"
                    max="1.0"
                    name="range" />
                <Hints>
                    <PropertyWidgetDecorator
                        type="GenericDecorator"
                        mode="visibility"
                        property="UserDefinedParameters"
                        value="1" />
                </Hints>
                <Documentation>
                    Set the maximum threshold on the dot product between the estimated shape's normal and
                    the point's normal, that is the cosine of the angle (cos(25°) = 0.9).
                </Documentation>
            </DoubleVectorProperty>
            <Hints>
                <ShowInMenu category="CGAL" />
            </Hints>
        </SourceProxy>

        <!-- ==================================================================== -->
        <SourceProxy
            name="CGALPolygonOffset"
            class="stkCGALPolygonOffset"
            label="CGAL Polygon Offset">

            <Documentation
                long_help="Offset a Polygon."
                short_help="Offset a Polygon.">
                Offset the edges of a simple 2D polygon (closed curve) to interior or exterior regions formed by the
                polygon.The polygon must belong to XY plane and input must be a closed curve.
            </Documentation>

            <InputProperty
                name="Input"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData" />
                </DataTypeDomain>
            </InputProperty>

            <IntVectorProperty
                name="OffsetType"
                command="SetOffsetType"
                number_of_elements="1"
                default_values="1">
                <EnumerationDomain name="enum">
                    <Entry
                        text="INTERIOR"
                        value="0" />
                    <Entry
                        text="EXTERIOR"
                        value="1" />
                </EnumerationDomain>
                <Documentation>
                    The direction of polygon offset. Interior means bounded region and Exterior means unbounded region formed by the polygon.
                </Documentation>
            </IntVectorProperty>

            <DoubleVectorProperty
                name="Offset"
                command="SetOffset"
                number_of_elements="1"
                default_values="2.0">
                <DoubleRangeDomain
                    min="0.0"
                    name="range" />
                <Documentation>
                    The value used to offset the polygon.
                </Documentation>
            </DoubleVectorProperty>

            <Hints>
                <ShowInMenu category="CGAL" />
            </Hints>
        </SourceProxy>

        <!-- ==================================================================== -->
        <SourceProxy
            name="CGAL3DConvexHull"
            class="stkCGAL3DConvexHull"
            label="CGAL 3D Convex Hull">
            <Documentation
                long_help="Create a 3D Convex Hull of the polygonal mesh."
                short_help="Create a 3D Convex Hull of the polygonal mesh.">
                Generate 3D Convex Hull from a Point Set Data.
 
                A subset S⊆R3 is convex if for any two points p and q in the set the line segment with endpoints p and q is contained in S.
                The convex hull of a set S is the smallest convex set containing S.
            </Documentation>

            <InputProperty
                name="InputPointSet"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet" />
                </DataTypeDomain>
                <Documentation>
                        Set the source point set.
                </Documentation>
            </InputProperty>

            <IntVectorProperty
                name="ConvexHullMethod"
                command="SetConvexHullMethod"
                number_of_elements="1"
                default_values="0">
                <EnumerationDomain name="enum">
                    <Entry
                        text="QuickHull"
                        value="0" />
                    <Entry
                        text="Delaunay3D"
                        value="1" />
                </EnumerationDomain>
                <Documentation>
                    Method used to generate Convex Hull 
                </Documentation>
            </IntVectorProperty>

            <Hints>
                <ShowInMenu category="CGAL" />
            </Hints>
        </SourceProxy>

        <!-- ==================================================================== -->
        <SourceProxy name="CGALSurfaceMeshTopology"
                     class="stkCGALSurfaceMeshTopology"
                     label="CGAL Surface Mesh Topology">
            <InputProperty name="Input"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData" />
                </DataTypeDomain>
                <InputArrayDomain
                name="VertexToCheckPointMaskArray"
                attribute_type="point"
                number_of_components="1" />
                </InputProperty>

                <StringVectorProperty
                name="VertexToCheckPointMaskName"
                command="SetVertexToCheckPointMaskName"
                label="Vertex To Check Point Mask"
                default_values="None"
                number_of_elements="1">
                <ArrayListDomain
                    name="array_list"
                    none_string="None"
                    input_domain_name="VertexToCheckPointMaskArray">
                    <RequiredProperties>
                        <Property
                            name="Input"
                            function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    
                </Documentation>
            </StringVectorProperty>

            <IntVectorProperty
