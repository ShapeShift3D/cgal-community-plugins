<ServerManagerConfiguration>
    <!-- ==================================================================== -->
    <ProxyGroup name="uv_parametrizations">
        <Proxy  name="CGALAbstractUVParametrization"
                class="vtkCGALAbstractUVParametrization">
            <Documentation>
                Abstract class that should not be used directly.
            </Documentation>

            <InputProperty name="Mesh"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData"/>
                </DataTypeDomain>
                <Documentation>
                    Set the input mesh.
                </Documentation>
            </InputProperty>

            <IntVectorProperty name="ScalingMode" command="SetScalingMode" number_of_elements="1" default_values="1">
                <EnumerationDomain name="enum">
                    <Entry text="Manual" value="1" />
                    <Entry text="Auto" value="2" />
                </EnumerationDomain>
                <Documentation>
					A manual scaling mode will give you the option to scale the surface according to a multiplier. 
                    Automatic scaling will force the surface area of the output to match the surface area of the input.
                </Documentation>
            </IntVectorProperty>

            <DoubleVectorProperty command="SetUVScaling"
                                  default_values="1.0"
                                  name="UVScaling"
                                  label="UV Scaling"
                                  number_of_elements="1">
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" 
                                             mode="visibility" 
                                             property="ScalingMode" 
                                             value="1">
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Scaling of UV map
                </Documentation>
            </DoubleVectorProperty>

        </Proxy>
        <!-- ==================================================================== -->
        <Proxy name="CGALARAPUVParametrization"
                 class="vtkCGALARAPUVParametrization"
                 label="ARAP #2"
                 base_proxygroup="uv_parametrizations"
                 base_proxyname="CGALAbstractUVParametrization">
        <Documentation long_help="Applies an ARAP UV Parametrization on a polygonal mesh."
                       short_help="Applies an ARAP UV Parametrization on a polygonal mesh."> </Documentation>

        <DoubleVectorProperty name="Lambda"
                              command="SetLambda"
                              number_of_elements="1"
                              animateable="0"
                              default_values="1000">
            <Documentation>
                Parameter to give importance to shape or angle preservation.
            </Documentation>
        </DoubleVectorProperty>

        <DoubleVectorProperty name="Tolerance"
                           command="SetTolerance"
                           number_of_elements="1"
                           default_values="1e-6">
            <Documentation>
                Minimal energy difference between two iterations for the minimization process to continue.
            </Documentation>
        </DoubleVectorProperty>

        <IntVectorProperty name="MaximumNumberOfIterations"
                           command="SetMaximumNumberOfIterations"
                           label="Maximum Number Of Iterations"
                           number_of_elements="1"
                           default_values="50">
            <Documentation>
                Maximal number of iterations in the energy minimization process.
            </Documentation>
        </IntVectorProperty>

            <IntVectorProperty command="SetSkipPostprocess"
                               name="SkipPostprocess"
                               label="Skip Postprocess"
                               default_values="0"
                               number_of_elements="1"
                               panel_visibility="advanced">
                <Documentation>
                    Removes postprocess of ARAP parametrization. Helpful when unwrap result has overlaps.
                </Documentation>
                <BooleanDomain name="bool" />
            </IntVectorProperty>
        </Proxy>
        <!-- ==================================================================== -->
        <Proxy name="CGALLSCMUVParametrization"
                 class="vtkCGALLSCMUVParametrization"
                 label="LSCM #2"
                 base_proxygroup="uv_parametrizations"
                 base_proxyname="CGALAbstractUVParametrization">
        <Documentation long_help="Applies a LSCM UV Parametrization on a polygonal mesh."
                       short_help="Applies a LSCM UV Parametrization on a polygonal mesh."> </Documentation>
        </Proxy>
    </ProxyGroup>
    <!-- ==================================================================== -->
    <ProxyGroup name="boolean_meshers">
        <Proxy name="CGALBoolean3DMesher"
                     class="vtkCGALBoolean3DMesher"
                     label="Boolean 3D #2">
            <Documentation long_help="Applies a boolean operation on two PolyData inputs."
                           short_help="Applies a boolean operation on two PolyData inputs."> </Documentation>

            <InputProperty name="InputMeshA"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the source data set.
                </Documentation>
            </InputProperty>

            <InputProperty name="InputMeshB"
                           port_index="1"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the source data set
                </Documentation>
            </InputProperty>

            <IntVectorProperty name="Mode"
                               command="SetMode"
                               number_of_elements="1"
                               default_values="0">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="Union"/>
                    <Entry value="2"
                           text="Intersection"/>
                    <Entry value="3"
                           text="Difference"/>
                    <Entry value="4"
                           text="Difference2"/>
                </EnumerationDomain>
                <Documentation>
                    This property indicates which mode will be used.
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty command="SetSkipPreconditions"
                               name="SkipPreconditions"
                               label="Skip Preconditions"
                               default_values="0"
                               number_of_elements="1"
                               panel_visibility="advanced">
                <Documentation>
                    Removes precondition checking
                </Documentation>
                <BooleanDomain name="bool" />
            </IntVectorProperty>

            <IntVectorProperty command="SetComputeSurfaceIntersection"
                               name="ComputeSurfaceIntersection"
                               label="Compute Surface Intersection"
                               default_values="0"
                               number_of_elements="1"
                               panel_visibility="advanced">
                <Documentation>
                    Calculates surface intersection between both inputs.
                </Documentation>
                <BooleanDomain name="bool" />
            </IntVectorProperty>

            <OutputPort name="Boolean Result"
                        index="0" />
            <OutputPort name="Intersection"
                        index="1" />

        </Proxy>
        <!-- ==================================================================== -->
        <Proxy name="CGALBoolean2DMesher"
                     class="vtkCGALBoolean2DMesher"
                     label="Boolean 2D #1">
            <Documentation long_help="Applies a 2d boolean operation on sets of PolyLine inputs."
                           short_help="Applies a 2d boolean operation on sets of PolyLine inputs."> </Documentation>

            <InputProperty name="InputPolyLineSetA"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the source data set.
                </Documentation>
            </InputProperty>

            <InputProperty name="InputPolyLineSetB"
                           port_index="1"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the source data set
                </Documentation>
            </InputProperty>

            <IntVectorProperty name="OperationMode"
                               command="SetOperationMode"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="Add"/>
                    <Entry value="2"
                           text="Intersect"/>
                    <Entry value="3"
                           text="A - B"/>
                    <Entry value="4"
                           text="B - A"/>
                    <Entry value="5"
                           text="Exclude Overlap"/>
                    <Entry value="6"
                           text="Complement"/>
                    <Entry value="7"
                           text="Intersect Complement"/>
                    <Entry value="8"
                           text="Exclusive Add"/>
                </EnumerationDomain>
                <Documentation>
                    This property indicates which operation mode will be used.
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty name="ComplementOf"
                               command="SetComplementOf"
                               label="Complement Of"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="A"/>
                    <Entry value="2"
                           text="B"/>
                </EnumerationDomain>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator"
                                             mode="visibility"
                                             property="OperationMode"
                                             value="6">
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Apply a complement on an input
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty name="Plane"
                               command="SetPlane"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="XY"/>
                    <Entry value="2"
                           text="YZ"/>
                    <Entry value="3"
                           text="XZ"/>
                </EnumerationDomain>
                <Documentation>
                    This property which plane to consider
                </Documentation>
            </IntVectorProperty>

            <StringVectorProperty name="PwhIdArrayName"
                                  command="SetPwhIdArrayName"
                                  number_of_elements="1"
                                  animateable="0"
                                  default_values="PolygonWithHolesId"
                                  panel_visibility="advanced">
                <Documentation>
                    Name of the Polygon With Holes Id Array Name
                </Documentation>
            </StringVectorProperty>

            <IntVectorProperty name="OneCell"
                               command="SetOneCell"
                               label="One Cell"
                               number_of_elements="1"
                               default_values="1">
                <BooleanDomain name="bool"/>
                <Documentation>
                    If true, every polygon will be composed of one cell.
                </Documentation>
            </IntVectorProperty>

            <Hints> 
                <ShowInMenu category="CGAL"/>
            </Hints>
        </Proxy>
        <!-- ==================================================================== -->
    </ProxyGroup>
    <!-- ==================================================================== -->
    <ProxyGroup name="loop_operators">
        <Proxy name="CGALPolygonOrientOperator"
                     class="vtkCGALPolygonOrientOperator"
                     label="CGAL Polygon Orient Operator">
            <Documentation long_help="Orders a polygon clockwise or counterclockwise."
                           short_help="Orders a polygon clockwise or counterclockwise."> </Documentation>

            <InputProperty name="InputPolyLine"
                           port_index="0"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the PolyLine A data set.
                </Documentation>
            </InputProperty>

            <IntVectorProperty name="Plane"
                               command="SetPlane"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="XY"/>
                    <Entry value="2"
                           text="YZ"/>
                    <Entry value="3"
                           text="XZ"/>
                </EnumerationDomain>
                <Documentation>
                    This property which plane to consider
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty name="InvertPolyLineOrientation"
                               command="SetInvertPolyLineOrientation"
                               label="Invert PolyLine Orientation"
                               number_of_elements="1"
                               default_values="0">
                <BooleanDomain name="bool" />
                <Documentation>
                    Inverts the PolyLine A orientation
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty name="ForcePolyLineOrientation"
                               command="SetForcePolyLineOrientation"
                               label="Force PolyLine Orientation"
                               number_of_elements="1"
                               default_values="0">
                <BooleanDomain name="bool"/>
                <Documentation>
                    Forces the PolyLine A orientation
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty name="PolyLineOrientation"
                               command="SetPolyLineOrientation"
                               label="Orientation"
                               number_of_elements="1"
                               default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="1"
                           text="Clockwise"/>
                    <Entry value="2"
                           text="Counterclockwise"/>
                </EnumerationDomain>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator"
                                             mode="visibility"
                                             property="ForcePolyLineOrientation"
                                             value="1">
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Choose the orientation to force
                </Documentation>
            </IntVectorProperty>

            <Hints> 
                <ShowInMenu category="CGAL"/>
            </Hints>
        </Proxy>
    </ProxyGroup>
    <!-- ==================================================================== -->
    <ProxyGroup name="point_classifiers">
        <Proxy name="CGALPolygonSetOrientedSideClassifier"
