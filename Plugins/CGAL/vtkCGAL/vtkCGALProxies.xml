                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the PointSet
                </Documentation>
            </InputProperty>

            <InputProperty name="InputPolyLineSet"
                           port_index="1"
                           command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
                <Documentation>
                    Set the polyLine set
